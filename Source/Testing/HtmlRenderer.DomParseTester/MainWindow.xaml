<Window x:Class="HtmlRenderer.DomParseTester.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HtmlRenderer.DomParseTester"
        mc:Ignorable="d"
        Title="DOM Test Tools" Height="250" Width="525">
    <StackPanel Orientation="Vertical">
        <Button Margin="5" Height="40" Click="OpenDomExplorer_Click">DOM Explorer</Button>
        <Button Margin="5" Height="40" Click="OpenDomComparer_Click">DOM Comparer</Button>
        <Button Margin="5" Height="40" Click="OpenKeywordsViewer_Click">Keywords Viewer</Button>
        <Button Margin="5" Height="40" Click="Exit_Click">Exit</Button>
    </StackPanel>
</Window>
